<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.juranoaa.threading">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AAMainActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".background.AABackgroundSampleActivity_"
            android:label="AABackgroundSampleActivity">
        </activity>
        <activity
            android:name=".background.BackgroundSampleActivity"
            android:label="BackgroundSampleActivity">
        </activity>
        <activity
            android:name=".uithread.AAUiThreadSampleActivity_"
            android:label="AAUiThreadSampleActivity">
        </activity>
        <activity
            android:name=".uithread.UiThreadSampleActivity"
            android:label="UiThreadSampleActivity">
        </activity>
    </application>

</manifest>
